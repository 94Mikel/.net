
<!--
    Para obtener los proyectos en la seccion de proyectos desde el index.
    Con IEnumerable obtenemos los proyectos que pasamos por model.
-->

@model IEnumerable<Proyecto>

@if (Model is not null){
    <div class="text-center" style="padding-top: 6rem;">
        <h1>Mis trabajos recientes</h1>
        <p>Aquí puedes ver algunos de mis trabajos más recientes.</p>
    </div>

    <partial name="_ListadoProyectos" model="Model" />
}